                }
            },
            "line": {
                "major": {
                    "color": "@AxisMajorColor"
                },
                "minor": {
                    "color": "@AxisMinorColor"
                }
            }
        },
        "grid": {
            "line": {
                "highContrast": {
                    "color": "#eeeeee"
                },
                "major": {
                    "color": "#eeeeee"
                },
                "minor": {
                    "color": "#eeeeee"
                }
            }
        },
        "referenceLine": {
            "label": {
                "name": {
                    "color": "@TextColor",
                    "fontSize": "@TextSize"
                }
            },
            "outOfBounds": {
                "color": "@TextColor",
                "backgroundColor": "#ffffff",
                "fontSize": "@TextSize"
            }
        },
        "legend": {
            "title": {
                "color": "@TextColor",
                "fontSize": "@TextSize"
            },
            "label": {
                "color": "@TextColor",
                "fontSize": "@TextSize"
